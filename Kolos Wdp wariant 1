#include<iostream>
#include<string>
using namespace std;

void zadanie1(){
	
	double a,b,c;
	cin>>a>>b>>c;
	
	double naj = max(a, b);
	double najnaj = max(naj,c);
	
	cout<<najnaj;
	
}

void zadanie2(){
	
	double a,b,c;
	cin>>a>>b>>c;
	
	if(a<0){
		 a = -a;
	}
	if(b<0){
		 b = -b;
	}
	if(c<0){
		 c = -c;
	} 
	
	void zadanie3(){
	
	double a,b,c;
	cin>>a,b,c;
	
	if(a<0){
		 a = -a;
	}
	if(b<0){
		 b = -b;
	}
	if(c<0){
		 c = -c;
	} 
	
	double suma;
	suma = a + b + c;
	cout<<suma;
}

int main(){
	int wybor;
	cin>>wybor;
	
	if(wybor==1){
	zadanie1();
	return 0;
	}
	if(wybor==2){
	zadanie2();
	return 0;
	}
	if(wybor==3){
	zadanie3();
	return 0;
	}	
	else(){
	cout<<"Error";
	}
}
